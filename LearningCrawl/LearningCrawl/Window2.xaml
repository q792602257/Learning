<Window x:Class="LearningCrawl.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:LearningCrawl"
        mc:Ignorable="d"
        Title="Window2" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="10"></RowDefinition>
            <RowDefinition Height="24"></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        
        <ProgressBar Name="PgBar3" Grid.Row="0" Height="10" Visibility="Collapsed"></ProgressBar>
        <Label Name="Label3" Grid.Row="1" MouseDoubleClick="Label3_OnMouseDoubleClick"></Label>
        <Image Name="Image3" Grid.Row="2"></Image>
    </Grid>
</Window>
